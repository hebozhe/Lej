def fun multiplyTups as:
    take tup[int] tupA, tup[int] tupB;
    expect tup[int];
    def nat lenA as len[tupA];
    def nat lenB as len[tupB];
    if not {lenA = lenB}:
        give [];
        \
    def tup[int] result as [];
    def nat i as 0;
    def nat j as 0;
    do this lenA times:
        result as result + [tupA[i] * tupB[j]];
        \
    give result;
    \